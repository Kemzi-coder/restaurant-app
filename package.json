{
	"name": "restaurant-app",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"test": "vitest --threads=false",
		"db:seed": "npx prisma db seed",
		"db:seed:dev": "dotenv -e .env.local -- npm run db:seed",
		"db:migrate:dev": "dotenv -e .env.local -- npx prisma migrate dev",
		"db:deploy": "npx prisma generate && npx prisma migrate reset --force && npx prisma migrate deploy",
		"vercel-build": "npm run db:deploy && next build"
	},
	"prisma": {
		"seed": "node prisma/seed.js"
	},
	"dependencies": {
		"@prisma/client": "^4.13.0",
		"classnames": "^2.3.2",
		"daisyui": "^2.51.6",
		"next": "13.4.1",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"devDependencies": {
		"@tailwindcss/typography": "^0.5.9",
		"@vitest/coverage-c8": "^0.31.0",
		"@vitest/ui": "^0.31.0",
		"autoprefixer": "^10.4.14",
		"dotenv-cli": "^7.2.1",
		"eslint": "^8.39.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-next": "^13.4.1",
		"eslint-config-prettier": "^8.8.0",
		"eslint-html-reporter": "^0.7.4",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"node-mocks-http": "^1.12.2",
		"postcss": "^8.4.23",
		"prettier": "^2.8.8",
		"prisma": "^4.13.0",
		"tailwindcss": "^3.3.2",
		"vitest": "^0.31.0"
	}
}
